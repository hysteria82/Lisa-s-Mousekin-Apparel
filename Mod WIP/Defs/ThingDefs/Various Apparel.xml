<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<!--BarMaidDress-->
<ThingDef ParentName="LisasMousekinApparelOutfits">
	<defName>Lisas_MousekinApparel_BarMaidDress</defName>
	<label>BarMaid Dress</label>
	<description>A functional yet flirty outfit designed for serving drinks and keeping patrons happy.</description>
	<graphicData>
		<texPath>Things/MousekinApparel/Clothes/MousekindBarmaidDress</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		<shaderType>CutoutComplex</shaderType>
	</graphicData>
	<uiIconPath>Things/MousekinApparel/Preview/BarMaidDress</uiIconPath>
	<apparel>
		<wornGraphicPath>Things/MousekinApparel/Clothes/MousekindBarMaidDress</wornGraphicPath>
		<useWornGraphicMask>true</useWornGraphicMask>
		<bodyPartGroups>
			<li>Torso</li>
			<li>Shoulders</li>
			<li>Arms</li>
			<li>Legs</li>
		</bodyPartGroups>
		<layers>
			<li>OnSkin</li>
		</layers>
		<defaultOutfitTags>
			<li>Worker</li>
		</defaultOutfitTags>
	</apparel>
	<costStuffCount><cloth>60</cloth>
	<leather>5</leather>
	</costStuffCount>
	<statBases>
		<WorkToMake>1000</WorkToMake>
		<Mass>1</Mass>
		<StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
		<StuffEffectMultiplierInsulation_Cold>0.25</StuffEffectMultiplierInsulation_Cold>
		<StuffEffectMultiplierInsulation_Heat>0.25</StuffEffectMultiplierInsulation_Heat>
		<EquipDelay>2</EquipDelay>
	</statBases>
	<equippedStatOffsets>
		<SocialImpact>0.1</SocialImpact>
		<CookSpeed>0.5</CookSpeed>
		<WorkSpeedGlobal>0.5</WorkSpeedGlobal>
		
		
	</equippedStatOffsets>
</ThingDef>

